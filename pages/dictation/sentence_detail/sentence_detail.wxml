<import src="/templates/page-loading.wxml" />
<template is="pageLoading" data="{{loading, loadProgress}}" />
<skeleton type="detail" loading="{{loading}}" rows="{{2}}" />
<view wx:if="{{!loading}}">
  <view class="list_container">
    <view class="explain_title">翻译</view>
    <view class="listening_content">
      <view class="listening_content_text" wx:if="{{translate}}">{{translate}}</view>
      <view class="listening_content_text" wx:else="{{!translate}}">暂无</view>
    </view>
  </view>
  <view class="list_container">
    <view class="explain_title">原文 </view>
    <view class="listening_content">
      <view class="listening_content_text" wx:if="{{questions}}">
        <view wx:for="{{questions}}" wx:key="item">{{item}}</view>
      </view>
      <view class="listening_content_text" wx:if="{{!questions}}">暂无</view>
    </view>
  </view>
</view>