
.set_label_con {
  margin-bottom: 15px;
}

.set_label {
  border: 1px solid #9abcfa;
  padding: 0 20px;
  height: 32px;
  line-height: 32px;
  border-radius: 30px;
  font-weight: bold;
  font-size: 14px;
  color: #00A6ED;
  margin-right: 10px;
}

.list_container {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border: 1px solid #00A6ED;
  color: #00A6ED;
  border-radius: 9px;
  box-sizing: border-box;
  width: 100%;
}

.list_title {
  font-size: 16px;
  width: 100px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.other_title {
  font-size: 12px;
}

.ml10 {
  margin-left: 10px;
}

.is_hidden {
  color: #a1a1a1;
}

.no_review {
  border-color: #b43040;
  color: #b43040;
}